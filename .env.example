# Copy to `.env` and fill in values as needed.

# OpenAI (required for any Qdrant upsert)
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Qdrant (optional; defaults to the docker-compose Qdrant when using `docker compose run`)
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=

# Docker file ownership (Linux only; prevents root-owned files in the repo)
UID=1000
GID=1000

